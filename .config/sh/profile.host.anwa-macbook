# vi: filetype=sh
# For Homebrew:
# shellcheck disable=SC1091
# shellcheck source=/Users/andreas/.rvm/scripts/rvm

PATH="$HOME/bin/dvd:\
/usr/local/mysql/bin:\
/usr/local/opt/helm@2/bin:\
$PATH:\
/usr/local/sbin:\
/usr/X11/bin"

export PATH

export TERM=screen-256color-italic
export VIM_APP_DIR=$HOME/local

locate() { mdfind "kMDItemDisplayName == '$@'wc"; }

[ -s "$HOME/.rvm/scripts/rvm" ] && . "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

