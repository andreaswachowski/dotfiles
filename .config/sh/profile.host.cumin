# Source default settings
# vi: filetype=sh
# $HOME-specific paths have already been set in the main dotfiles-profile
# Just append the machine-specific ones now
export PATH=$PATH:\
/usr/bin/X11:\
/usr/local/sbin:\
/usr/local/mysql/bin

umask 022

# /opt/etc/profile sets TERM=xterm
# Adjust to screen-256color for better vim/tmux experience
export TERM=screen-256color
